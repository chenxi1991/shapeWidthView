<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ShapeWidthView">
        <attr name="sw_max_value" format="float" />
        <attr name="sw_min_value" format="float" />
        <attr name="sw_current_value" format="float" />
        <attr name="sw_board_color" format="color" />
        <attr name="sw_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ShapeSelectedColorView">
        <attr name="sc_board_color" format="color" />
        <attr name="sc_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ShapeColorStateView">
        <attr name="ss_board_color" format="color" />
        <attr name="ss_fill_color" format="color" />
        <attr name="ss_current_state" format="integer" />
    </declare-styleable>
</resources>